<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/label_developer">
    <!-- at the bottom -->
    <ListPreference
        android:defaultValue="@string/default_pref_csv_encoding"
        android:entries="@array/prefs_csv_encoding_values"
        android:entryValues="@array/prefs_csv_encoding_values"
        android:key="@string/pref_csv_encoding"
        android:summary="@string/label_prefs_csv_encoding_sum"
        android:title="@string/label_prefs_csv_encoding" />
    <SwitchPreference
        android:defaultValue="@string/default_pref_testsdekstop"
        android:key="@string/pref_testsdekstop"
        android:summary="@string/label_prefs_testsdekstop_sum"
        android:title="@string/label_prefs_testsdekstop" />

    <SwitchPreference
        android:defaultValue="@string/default_pref_log_on"
        android:key="@string/pref_log_on"
        android:summary="@string/label_prefs_log_on_sum"
        android:title="@string/label_prefs_log_on" />

    <EditTextPreference
        android:defaultValue="@string/default_pref_log_max_line"
        android:key="@string/pref_log_max_line"
        android:numeric="integer"
        android:summary="@string/label_prefs_log_max_line_sum"
        android:title="@string/label_prefs_log_max_line" />
    <Preference
        android:key="log_viewer"
        android:persistent="false"
        android:summary="@string/label_prefs_log_viewer_sum"
        android:title="@string/title_log_viewer" />

</PreferenceScreen>
